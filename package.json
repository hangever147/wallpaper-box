{
  "name": "wallpaper-box",
  "private": true,
  "type": "commonjs",
  "version": "0.0.0",
  "main": "./dist-electron/main.js",
  "description": "wallpaper box",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently --kill-others-on-fail \"yarn dev:web \" \"wait-on tcp:1234 && yarn dev:electron\"",
    "dev:web": "vite",
    "dev:electron": "cross-env IS_DEV=true node ./dev-electron.mjs",
    "dev:sort": "vite --mode sort",
    "preview": "vite preview",
    "build": "yarn make",
    "build:web": "tsc && vite build",
    "build:electron": "rollup -c",
    "electron": "electron .",
    "package": "electron-forge package",
    "make": "sudo rm -rf ./out && yarn build:web && yarn build:electron && electron-forge make",
    "electron:start": "rollup -c && cross-env IS_DEV=false electron-forge start",
    "electron:dev": "rollup -c && cross-env IS_DEV=true electron-forge start"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.9.1",
    "@ant-design/icons": "^5.0.1",
    "ahooks": "^3.7.4",
    "antd": "^5.1.2",
    "axios": "^1.2.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.1.0",
    "fs-extra": "^11.1.0",
    "lodash": "^4.17.21",
    "node-stdlib-browser": "^1.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.6.1",
    "redux": "^4.2.0",
    "wallpaper": "^6.1.1",
    "win-win-api": "^0.0.5",
    "win32-ffi": "0.0.6-alpha.5"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.1.1",
    "@electron-forge/maker-deb": "^6.1.1",
    "@electron-forge/maker-dmg": "^6.1.1",
    "@electron-forge/maker-rpm": "^6.1.1",
    "@electron-forge/maker-squirrel": "^6.1.1",
    "@electron-forge/maker-zip": "^6.1.1",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "chokidar": "^3.5.3",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "electron": "24.2.0",
    "postcss": "^8.4.20",
    "rollup": "^3.8.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-typescript2": "^0.34.1",
    "sass": "^1.57.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "unplugin-auto-import": "^0.12.1",
    "vite": "^4.0.0",
    "vite-plugin-esmodule": "^1.4.4",
    "vite-plugin-optimizer": "^1.4.2",
    "vite-plugin-svg-icons": "^2.0.1",
    "wait-on": "^7.0.1"
  }
}
